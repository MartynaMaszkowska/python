  def moj_range(*args):
    count=len(args)
    result=[]
    if count==1:
      i=0
      while i<args[0]:
        result.append(i)
        i+=1
    if count==2:
      i=args[0]
      while i<args[1]:
        result.append(i)
        i+=1
    if count==3:
      i=args[0]
      while i<args[1]:
        result.append(i)
        i+=args[2]
    return result

